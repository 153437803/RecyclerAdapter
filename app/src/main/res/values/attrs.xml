<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--圆角图片的属性-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <!-- 标题模板自定义属性 -->
    <declare-styleable name="TemplateTitle">
        <!-- 标题文案 -->
        <attr name="titleText" format="string" />
        <!-- 是否有back按钮 -->
        <attr name="canBack" format="boolean" />
        <!-- back按钮文案 -->
        <attr name="backText" format="string" />
        <!-- 右侧更多功能按钮文字 -->
        <attr name="moreText" format="string" />
    </declare-styleable>

    <!-- 条形控制控件自定义属性 -->
    <declare-styleable name="LineControllerView">
        <!-- 名称 -->
        <attr name="name" format="string" />
        <!-- 内容或当前状态 -->
        <attr name="content" format="string" />
        <!-- 是否是列表中最后一个 -->
        <attr name="isBottom" format="boolean" />
        <!-- 是否可以跳转 -->
        <attr name="canNav" format="boolean" />
        <!-- 是否是开关 -->
        <attr name="isSwitch" format="boolean" />
    </declare-styleable>

    <!-- BannerLayout -->
    <declare-styleable name="BannerLayout">
        <attr name="selectedIndicatorColor" format="color|reference" />
        <attr name="unSelectedIndicatorColor" format="color|reference" />
        <attr name="indicatorShape" format="enum">
            <enum name="rect" value="0" />
            <enum name="oval" value="1" />
        </attr>

        <attr name="selectedIndicatorHeight" format="dimension|reference" />
        <attr name="selectedIndicatorWidth" format="dimension|reference" />

        <attr name="unSelectedIndicatorHeight" format="dimension|reference" />
        <attr name="unSelectedIndicatorWidth" format="dimension|reference" />

        <attr name="indicatorPosition" format="enum">
            <enum name="centerBottom" value="0" />
            <enum name="rightBottom" value="1" />
            <enum name="leftBottom" value="2" />
            <enum name="centerTop" value="3" />
            <enum name="rightTop" value="4" />
            <enum name="leftTop" value="5" />
        </attr>

        <attr name="indicatorSpace" format="dimension|reference" />
        <attr name="indicatorMargin" format="dimension|reference" />
        <attr name="autoPlayDuration" format="integer|reference" />
        <attr name="scrollDuration" format="integer|reference" />
        <attr name="isAutoPlay" format="boolean" />
        <attr name="isCanLoop" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextPlusView">
        <attr name="left_height" format="dimension" />
        <attr name="left_width" format="dimension" />
        <attr name="right_height" format="dimension" />
        <attr name="right_width" format="dimension" />
        <attr name="top_height" format="dimension" />
        <attr name="top_width" format="dimension" />
        <attr name="bottom_height" format="dimension" />
        <attr name="bottom_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ProgressLoading">
        <attr name="pl_indeterminate" format="boolean" />
        <attr name="pl_barColor" format="color" />
        <attr name="pl_rimColor" format="color" />
        <attr name="pl_rimWidth" format="dimension" />
        <attr name="pl_spinSpeed" format="float" />
        <attr name="pl_barSpinCycleTime" format="integer" />
        <attr name="pl_circleRadius" format="dimension" />
        <attr name="pl_fillRadius" format="boolean" />
        <attr name="pl_barWidth" format="dimension" />
        <attr name="pl_linearProgress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ViderPlayer">
        <attr name="is_auto_play" format="boolean" />
    </declare-styleable>

    <declare-styleable name="EmBackgroundTintHelper">
        <attr name="backgroundTint" />
        <attr name="backgroundTintMode" />
    </declare-styleable>

    <!-- 末行对齐方式 -->
    <declare-styleable name="TextAlignView">
        <attr name="align" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RippleView">
        <attr name="rv_color" format="color" />
        <attr name="rv_alpha_factor" format="float" />
        <attr name="rv_hover" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DotParentView">
        <attr name="dpv_margin_top" format="dimension" />
        <attr name="dpv_margin_left" format="dimension" />
        <attr name="dpv_tips_count" format="integer" />
        <attr name="dpv_is_show" format="boolean" />
        <attr name="dpv_color" format="color" />
        <attr name="dpv_horizontal_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DotTextView">
        <attr name="dot_is_follow_text" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TagLayout">
        <attr name="tag_gravity" format="integer">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StateButton">

        <!--text color-->
        <attr name="normalTextColor" format="color|reference" />
        <attr name="pressedTextColor" format="color|reference" />
        <attr name="unableTextColor" format="color|reference" />

        <!--stroke width and color, dash width, dash gap-->
        <attr name="strokeDashWidth" format="dimension|reference" />
        <attr name="strokeDashGap" format="dimension|reference" />
        <attr name="normalStrokeWidth" format="dimension|reference" />
        <attr name="pressedStrokeWidth" format="dimension|reference" />
        <attr name="unableStrokeWidth" format="dimension|reference" />
        <attr name="normalStrokeColor" format="color|reference" />
        <attr name="pressedStrokeColor" format="color|reference" />
        <attr name="unableStrokeColor" format="color|reference" />

        <!--background color-->
        <attr name="normalBackgroundColor" format="color|reference" />
        <attr name="pressedBackgroundColor" format="color|reference" />
        <attr name="unableBackgroundColor" format="color|reference" />

        <!--background radius-->
        <attr name="radius" format="dimension|reference" />
        <attr name="round" format="boolean|reference" />

        <!--animation duration-->
        <attr name="animationDuration" format="integer|reference" />

    </declare-styleable>

    <declare-styleable name="BubbleView">
        <attr name="bv_arrow_width" format="dimension" />
        <attr name="bv_angle" format="dimension" />
        <attr name="bv_arrow_height" format="dimension" />
        <attr name="bv_arrow_margin_top" format="dimension" />
        <attr name="bv_color" format="color" />
        <attr name="bv_arrow_local" format="enum">
            <enum name="left" value="0x00" />
            <enum name="right" value="0x01" />
            <enum name="top" value="0x02" />
            <enum name="bottom" value="0x03" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AbstractImageView">
        <attr name="aiv_borderWidth" format="dimension|reference" />
        <attr name="aiv_borderColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="leftTopRadius" format="dimension|reference" />
        <attr name="rightTopRadius" format="dimension|reference" />
        <attr name="rightBottomRadius" format="dimension|reference" />
        <attr name="leftBottomRadius" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="TextTabLayout">

        <!-- 设置显示器颜色 -->
        <attr name="ttl_indicator_color" format="color" />
        <!-- 设置显示器高度 -->
        <attr name="ttl_indicator_height" format="dimension" />
        <!-- 设置显示器固定宽度 -->
        <attr name="ttl_indicator_width" format="dimension" />
        <!-- 设置显示器margin,当indicator_width大于0,无效 -->
        <attr name="ttl_indicator_margin_left" format="dimension" />
        <attr name="ttl_indicator_margin_top" format="dimension" />
        <attr name="ttl_indicator_margin_right" format="dimension" />
        <attr name="ttl_indicator_margin_bottom" format="dimension" />
        <!-- 设置显示器圆角弧度-->
        <attr name="ttl_indicator_corner_radius" format="dimension" />
        <!-- 设置显示器上方还是下方,只对圆角矩形有用-->
        <attr name="ttl_indicator_gravity" format="enum">
            <enum name="TOP" value="48" />
            <enum name="BOTTOM" value="80" />
        </attr>
        <!-- 设置显示器为常规|三角形|背景色块|-->
        <attr name="ttl_indicator_style" format="enum">
            <enum name="NORMAL" value="0" />
            <enum name="TRIANGLE" value="1" />
            <enum name="BLOCK" value="2" />
        </attr>
        <!-- 设置显示器长度与title一样长,只有在STYLE_NORMAL并且indicatorWidth小于零有效-->
        <attr name="ttl_indicator_width_equal_title" format="boolean" />
        <!-- 设置显示器支持动画-->
        <attr name="ttl_indicator_anim_enable" format="boolean" />
        <!-- 设置显示器动画时间-->
        <attr name="ttl_indicator_anim_duration" format="integer" />
        <!-- 设置显示器支持动画回弹效果-->
        <attr name="ttl_indicator_bounce_enable" format="boolean" />

        <!-- underline -->
        <!-- 设置下划线颜色 -->
        <attr name="ttl_underline_color" format="color" />
        <!-- 设置下划线高度 -->
        <attr name="ttl_underline_height" format="dimension" />
        <!-- 设置下划线上方还是下方-->
        <attr name="ttl_underline_gravity" format="enum">
            <enum name="TOP" value="48" />
            <enum name="BOTTOM" value="80" />
        </attr>

        <!-- divider -->
        <!-- 设置分割线颜色 -->
        <attr name="ttl_divider_color" format="color" />
        <!-- 设置分割线宽度 -->
        <attr name="ttl_divider_width" format="dimension" />
        <!-- 设置分割线的paddingTop和paddingBottom -->
        <attr name="ttl_divider_padding" format="dimension" />

        <!-- tab -->
        <!-- 设置tab的paddingLeft和paddingRight -->
        <attr name="ttl_tab_padding" format="dimension" />
        <!-- 设置tab大小等分 -->
        <attr name="ttl_tab_space_equal" format="boolean" />
        <!-- 设置tab固定大小 -->
        <attr name="ttl_tab_width" format="dimension" />

        <attr name="ttl_text_color_select" format="color" />
        <attr name="ttl_text_color_normal" format="color" />
        <attr name="ttl_text_size" format="dimension" />
        <attr name="ttl_text_bold" format="enum">
            <enum name="NONE" value="0" />
            <enum name="SELECT" value="1" />
            <enum name="BOTH" value="2" />
        </attr>
        <!-- 设置字体全大写 -->
        <attr name="ttl_text_capital_letter" format="boolean" />

    </declare-styleable>

    <declare-styleable name="PullRefreshLoadLayout">
        <attr name="prll_pull_enable_down" format="boolean" />
        <attr name="prll_pull_enable_up" format="boolean" />
        <attr name="prll_pull_color" format="color|reference" />
        <attr name="prll_pull_ignore_range" format="dimension" />
        <attr name="prll_pull_effective_range" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TextExpandView">
        <attr name="tev_close_max_lines" format="reference|integer" />
        <attr name="tev_open_hint_text" format="reference|string" />
        <attr name="tev_close_hint_text" format="reference|string" />
        <attr name="tev_close_hint_text_color" format="reference|color" /><!--"to expand" hint string, default is "Expand"-->
        <attr name="tev_close_hint_background_color" format="reference|color" /><!--background color if "toExpandHint" pressed-->
        <attr name="tev_toogle_enable" format="reference|boolean" />
        <attr name="tev_EllipsisHint" format="reference|string" /><!--default is ..-->
        <attr name="tev_GapToExpandHint" format="reference|string" /><!--gap between "toExpandHint" and "trimmed text"-->
        <attr name="tev_GapToShrinkHint" format="reference|string" /><!--gap between "toShrinkHint" and "trimmed text"-->
        <attr name="tev_ToExpandHintShow" format="reference|boolean" /><!--if show "toExpandHint"-->
        <attr name="tev_ToShrinkHintShow" format="reference|boolean" /><!--if show "toShrinkHint"-->
        <attr name="tev_ToShrinkHintColor" format="reference|color" /><!--text color of "toShrinkHint"-->
        <attr name="tev_ToShrinkHintColorBgPressed" format="reference|color" /><!--background color if "toShrinkHint" pressed-->
        <attr name="tev_InitState" format="enum"><!--init state, default is shrink-->
            <enum name="shrink" value="0" />
            <enum name="expand" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="GiftParentLayout">
        <attr name="gpl_sub_count" format="integer" />
    </declare-styleable>

    <declare-styleable name="TextEdgeView">
        <attr name="tev_shadow_inner_color" format="color" />
        <attr name="tev_shadow_inner_radius" format="float" />
        <attr name="tev_shadow_inner_dx" format="float" />
        <attr name="tev_shadow_inner_dy" format="float" />
        <attr name="tev_shadow_outer_color" format="color" />
        <attr name="tev_shadow_outer_radius" format="float" />
        <attr name="tev_shadow_outer_dx" format="float" />
        <attr name="tev_shadow_outer_dy" format="float" />
        <attr name="tev_foreground" format="reference|color" />
        <attr name="tev_stroke_width" format="float" />
        <attr name="tev_stroke_miter" format="float" />
        <attr name="tev_stroke_color" format="color" />
        <attr name="tev_stroke_join_style">
            <enum name="miter" value="0" />
            <enum name="bevel" value="1" />
            <enum name="round" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="StickyLayout">
        <attr name="sl_offset_tab" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="progress_value" format="integer" />
        <attr name="progress_color" format="color" />
        <attr name="background_color" format="color" />
        <attr name="progress_width" format="dimension" />
        <attr name="background_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LineChartView">
        <attr name="lcv_board_enable" format="boolean" />
        <attr name="lcv_frame_line_width" format="dimension" />
        <attr name="lcv_frame_color" format="color" />
        <attr name="lcv_frame_line_x_color" format="color" />
        <attr name="lcv_frame_line_x_enable" format="boolean" />
        <attr name="lcv_frame_line_y_color" format="color" />
        <attr name="lcv_frame_line_y_enable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ChooseView">
        <attr name="type_text" format="reference|string" />
        <attr name="type_text_size" format="reference|dimension" />
        <attr name="type_text_color" format="color" />
        <attr name="result_text" format="reference|string" />
        <attr name="result_text_size" format="reference|dimension" />
        <attr name="result_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ArrowView">
        <attr name="av_orientation">
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
            <enum name="left" value="3" />
            <enum name="right" value="4" />
        </attr>
        <attr name="av_color" format="color" />
        <attr name="av_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TextLineView">
        <attr name="tlv_orientation">
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
            <enum name="left" value="3" />
            <enum name="right" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="lv_bar_color" format="color" />
        <attr name="lv_bar_num" format="integer" />
    </declare-styleable>

    <declare-styleable name="LineLinearLayout">
        <attr name="lll_padding_left" format="dimension|reference" />
        <attr name="lll_padding_right" format="dimension|reference" />
        <attr name="lll_padding_top" format="dimension|reference" />
        <attr name="lll_padding_bottom" format="dimension|reference" />
        <attr name="lll_location_top" format="boolean" />
        <attr name="lll_location_bottom" format="boolean" />
        <attr name="lll_location_left" format="boolean" />
        <attr name="lll_location_right" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LineRelativeLayout">
        <attr name="lrl_padding_left" format="dimension|reference" />
        <attr name="lrl_padding_right" format="dimension|reference" />
        <attr name="lrl_padding_top" format="dimension|reference" />
        <attr name="lrl_padding_bottom" format="dimension|reference" />
        <attr name="lrl_location_top" format="boolean" />
        <attr name="lrl_location_bottom" format="boolean" />
        <attr name="lrl_location_left" format="boolean" />
        <attr name="lrl_location_right" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LineTextView">
        <attr name="ltv_padding_left" format="dimension|reference" />
        <attr name="ltv_padding_right" format="dimension|reference" />
        <attr name="ltv_padding_top" format="dimension|reference" />
        <attr name="ltv_padding_bottom" format="dimension|reference" />
        <attr name="ltv_paint_bold" format="boolean" />
        <attr name="ltv_location_top" format="boolean" />
        <attr name="ltv_location_bottom" format="boolean" />
        <attr name="ltv_location_left" format="boolean" />
        <attr name="ltv_location_right" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LineImageView">
        <attr name="liv_padding_left" format="dimension|reference" />
        <attr name="liv_padding_right" format="dimension|reference" />
        <attr name="liv_padding_top" format="dimension|reference" />
        <attr name="liv_padding_bottom" format="dimension|reference" />
        <attr name="liv_location_top" format="boolean" />
        <attr name="liv_location_bottom" format="boolean" />
        <attr name="liv_location_left" format="boolean" />
        <attr name="liv_location_right" format="boolean" />
    </declare-styleable>

</resources>